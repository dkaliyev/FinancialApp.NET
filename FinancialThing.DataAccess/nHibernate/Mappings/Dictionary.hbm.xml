<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="FinancialThing.Models.Dictionary, FinancialThing.Models" table="dictionary">
    <id name="Id" type="guid">
      <generator class="guid" />
    </id>
    <property name="DisplayName">
      <column name="DisplayName" sql-type="varchar" not-null="true" />
    </property>
    <property name="Code">
      <column name="Code" sql-type="varchar" not-null="true" />
    </property>
    <property name="ParentCode">
      <column name="ParentCode" sql-type="varchar" not-null="true" />
    </property>
    <property name="Order">
      <column name="OrderN" sql-type="varchar" not-null="true" />
    </property>
  </class>
</hibernate-mapping>